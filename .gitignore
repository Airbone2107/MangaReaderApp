/MangaReaderFrontend/android/app/debug
/MangaReaderFrontend/android/app/profile
/MangaReaderFrontend/android/app/release

# Android Keystore file
/MangaReaderFrontend/android/app/*.keystore
/MangaReaderFrontend/android/app/*.jks
/MangaReaderFrontend/android/app/*.p12
/MangaReaderFrontend/android/key.properties